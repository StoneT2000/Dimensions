{
  "name": "dimensions-ai",
  "version": "1.0.19",
  "description": "A generic, multi-level ai competition framework",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "dev-install": "npm install && cd src/Station/web",
    "test": "jest tests --collectCoverage",
    "test-watch": "jest --watch tests --collectCoverage",
    "docs": "typedoc --exclude ./src/Station/web/ --out docs src",
    "watch": "tsc -w",
    "dev-frontend": "npm run start --prefix=src/Station/web",
    "build": "tsc && npm run buildStation",
    "buildStation": "npm run build --prefix=src/Station/web && rm -rf lib/Station/web && mkdir lib/Station/web && cp -r src/Station/web/build lib/Station/web/"
  },
  "keywords": [
    "AI",
    "competition",
    "framework"
  ],
  "author": "StoneT2000",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.9.4",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@dimensions-ai/designs-halite3": "^1.0.2",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.31",
    "jest": "24.9.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/StoneT2000/Dimensions.git"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "build"
    ]
  }
}
